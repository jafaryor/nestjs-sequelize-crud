{
  "name": "nest-blog-api",
  "version": "0.0.1",
  "description": "Nest + sequelize + JWT + Jest + Swagger",
  "author": "Jafar",
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "nest.js",
    "sequelize",
    "sequelize-typescript",
    "orm",
    "nodejs",
    "node.js",
    "node",
    "typescript",
    "jwt",
    "jsonwebtoken",
    "passport",
    "swagger",
    "jest"
  ],
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "npm run build && node_modules/.bin/sequelize db:migrate",
    "db:drop": "npm run build && node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed-dev": "npm run build && node_modules/.bin/sequelize db:seed:all",
    "db:truncate": "npm run build && node_modules/.bin/sequelize db:seed:undo:all",
    "db:reset": "npm run db:drop && npm run db:migrate && npm run db:seed-dev"
  },
  "dependencies": {
    "@nestjs/common": "^7.1.1",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.1.1",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.1.1",
    "@nestjs/swagger": "^4.6.0",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.5",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^1.0.0",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.2.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.1.1",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "2.0.9",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-node": "^8.10.2",
    "tsc-watch": "^4.2.9",
    "tsconfig-paths": "3.9.0",
    "tslint": "^6.1.3",
    "typescript": "3.9.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
